{
  "type": "mysql",
  "host": "localhost",
  "port": 3306,
  "username": "your_username",
  "password": "your_password",
  "database": "your_database_name",
  "synchronize": true,
  "logging": true,
  "entities": [
    "src/model/**/*.ts"
  ],
  "migrations": [
    "src/migrations/**/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/models",
    "migrationsDir": "src/migrations"
  }
}